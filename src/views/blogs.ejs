
<div class="row">

    <%blogs.forEach((blog)=>{%>
        <div class="col-md-6">
            <div class="card d-flex gap-4 flex-row p-3 ">
                <div class="image-wrapper">
                    <img src="<%=blog.img%>" class="card-img-top" alt="...">
                </div>
                <div class="card-body">
                  <h5 class="card-title"><%=blog.title%></h5>
                  <p class="card-text"><%=blog.description%></p>
                </div>
              </div>
        </div>
    <%})%>

</div>